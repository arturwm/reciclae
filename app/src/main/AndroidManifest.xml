<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reciclae">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ResetarSenha"></activity>
        <activity android:name=".activity.Perfil" />
        <activity android:name=".activity.Comprar" />
        <activity android:name=".activity.SegundoCadastrar" />
        <activity android:name=".activity.Vender" />
        <activity android:name=".activity.Cadastrar" />
        <activity android:name=".activity.MainMenu" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>